{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@my-projects-nx/question-randomizer/shell/feature": [
        "libs/question-randomizer/shell/feature/src/index.ts"
      ],
      "@my-projects-nx/question-randomizer/shell/ui/header": [
        "libs/question-randomizer/shell/ui/header/src/index.ts"
      ],
      "@my-projects-nx/question-randomizer/shell/ui/layout": [
        "libs/question-randomizer/shell/ui/layout/src/index.ts"
      ],
      "@my-projects-nx/shared/ui-crt": ["libs/shared/ui-crt/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
